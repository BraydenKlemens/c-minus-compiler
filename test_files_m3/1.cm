void main(void) { 
  int x; int fac; 
  x = input(); 
  fac = 1;
  if (x == 1) {
    output(1);
    return;
  } else {
    while (x > 1) { 
      fac = fac * x; 
      x = x - 1; 
    } 
  }
  output(fac); 
}