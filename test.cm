void foo2 (int a[], int x) {
  int i;
  i = 0;
  while (i < 5) {
    a[i] = a[i] * x;
    output(a[i]);
    i = i + 1;
  }
}

void foo (int a[], int x) {
  foo2(a, x);
}

void main(void) {
  int a[5];
  int i;
  i = 0;
  while (i < 5) {
    a[i] = i;
    i = i + 1;
  }
  foo(a, 10);
  i = 0;
  while (i < 5) {
    a[i] = a[i] * 10;
    output(a[i]);
    i = i + 1;
  }
}